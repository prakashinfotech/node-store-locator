<div class="card">
    <div class="card-body">
        <div class="card-title">Store Detail</div>
        <div class="form-group">
            <label>Business name</label>: <%=currentStore[0].name%>
        </div>
        <div class="form-group">
            <label>Category</label>: <%=currentStore[0].category%>
        </div>
        <div class="form-group">
            <label>Photo URL</label>: <%=currentStore[0].photoUrl%>
        </div>
        <div class="form-group">
            <label>About</label>: <%=currentStore[0].description%>
        </div>
        <div class="form-group">
            <label>Address</label>: <%=currentStore[0].address%>
        </div>
        <div class="form-group">
            <label>Lattitude</label>: <%=currentStore[0].latitude%>
        </div>
        <div class="form-group">
            <label>Longitude</label>: <%=currentStore[0].longitue%>
        </div>
    </div>
</div>